{
  "buildCommand": "npm run build:ssg",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "cliniquedentairedabia.com" }
      ],
      "destination": "https://www.cliniquedentairedabia.com/:path*",
      "statusCode": 308
    },
    {
      "source": "/clinique-dentaire-dakar",
      "destination": "/cabinet-dentaire-dakar",
      "permanent": true
    },
    { "source": "/services", "destination": "/all-competences", "permanent": true }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/api/sitemap" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
